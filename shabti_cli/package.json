{
	"name": "shabti_cli",
	"version": "0.1.0-alpha.3",
	"module": "index.ts",
	"type": "module",
	"private": true,
	"devDependencies": {
		"@types/bun": "latest",
		"@types/cli-progress": "^3.11.6"
	},
	"peerDependencies": {
		"typescript": "^5"
	},
	"dependencies": {
		"@infosecinnovations/shabti-api-client": "workspace:*",
		"cli-progress": "^3.12.0",
		"commander": "^14.0.2",
		"dotenv": "^16.5.0",
		"openid-client": "^6.4.2"
	},
	"scripts": {
		"cli_dev_api": "API_URL=http://localhost:8000 bun run index.ts",
		"cli": "bun run index.ts",
		"build_win": "bun build ./index.ts --compile --target=bun-windows-x64 --outfile dist/windows/shabti_cli --define 'process.env.SHABTI_ENVIRONMENT_TYPE=\"executable\"'",
		"build_linux": "bun build ./index.ts --compile --target=bun-linux-x64 --outfile dist/linux/shabti_cli --define 'process.env.SHABTI_ENVIRONMENT_TYPE=\"executable\"'",
		"build_mac": "bun build ./index.ts --compile --target=bun-darwin-arm64 --outfile dist/mac/shabti_cli --define 'process.env.SHABTI_ENVIRONMENT_TYPE=\"executable\"'"
	}
}
